{
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.gitignore": true,
        "**/.gitattributes": true,
        "**/.gitmodules": true,
        "**/*.booproj": true,
        "**/*.pidb": true,
        "**/*.suo": true,
        "**/*.user": true,
        "**/*.userprefs": true,
        "**/*.unityproj": true,
        "**/*.dll": true,
        "**/*.exe": true,
        "**/*.pdf": true,
        "**/*.mid": true,
        "**/*.midi": true,
        "**/*.wav": true,
        "**/*.gif": true,
        "**/*.ico": true,
        "**/*.jpg": true,
        "**/*.jpeg": true,
        "**/*.png": true,
        "**/*.psd": true,
        "**/*.tga": true,
        "**/*.tif": true,
        "**/*.tiff": true,
        "**/*.3ds": true,
        "**/*.3DS": true,
        "**/*.fbx": true,
        "**/*.FBX": true,
        "**/*.lxo": true,
        "**/*.LXO": true,
        "**/*.ma": true,
        "**/*.MA": true,
        "**/*.obj": true,
        "**/*.OBJ": true,
        "**/*.asset": true,
        "**/*.cubemap": true,
        "**/*.flare": true,
        "**/*.mat": true,
        "**/*.meta": true,
        "**/*.prefab": true,
        "**/*.unity": true,
        "**/*.vsconfig": true,
        "build/": true,
        "Build/": true,
        "Library/": true,
        "library/": true,
        "obj/": true,
        "Obj/": true,
        "ProjectSettings/": true,
        "temp/": true,
        "Temp/": true,
        "Logs/": true,
        "Packages/": true,
        "UserSettings/": true,
        ".idea/": true,
        ".vs/": true
    },
    "editor.autoClosingQuotes": "beforeWhitespace",
    "editor.codeLens": false,
    "editor.trimAutoWhitespace": false,
    "editor.minimap.enabled": false,
    "editor.insertSpaces": true,
    "window.zoomLevel": 0,
    "files.encoding": "utf8",
    "files.eol": "\n",
    "files.trimTrailingWhitespace": false,
    "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
    "dotnet.completion.triggerCompletionInArgumentLists": false,
    "omnisharp.enableEditorConfigSupport": true,
    "workbench.colorCustomizations": {
        "[Default Light+]": {
            "activityBar.background": "#dddddd",
            "activityBar.foreground": "#333333"
        },
        "[One Monokai]": {
            "editorHoverWidget.highlightForeground": "#2aaaff",
            "editorLineNumber.foreground": "#818181",
        }
    },
    "editor.semanticTokenColorCustomizations":{
        "[One Monokai]": {
            "enabled": true
        }
    },
    "dotnet.defaultSolution": "unity_swfParse.sln",
    "dotnet.formatting.organizeImportsOnFormat": true
}